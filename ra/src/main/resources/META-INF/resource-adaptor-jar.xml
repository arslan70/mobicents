<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE resource-adaptor-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Resource Adaptor 1.1//EN"
                                      "http://java.sun.com/dtd/slee-resource-adaptor-jar_1_1.dtd">
<resource-adaptor-jar>
    <resource-adaptor>
        <description/>
        <resource-adaptor-name>Akka-RA</resource-adaptor-name>
        <resource-adaptor-vendor>rockville</resource-adaptor-vendor>
        <resource-adaptor-version>1.0</resource-adaptor-version>
        <resource-adaptor-type-ref>
            <resource-adaptor-type-name>akka-ra-type</resource-adaptor-type-name>
            <resource-adaptor-type-vendor>rockville</resource-adaptor-type-vendor>
            <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
        </resource-adaptor-type-ref>
        <library-ref>
            <library-name>Akka-ResourceAdaptor-akka-library-library</library-name>
            <library-vendor>org.mobicents</library-vendor>
            <library-version>1.0</library-version>
        </library-ref>
        <resource-adaptor-classes>
            <resource-adaptor-class supports-active-reconfiguration="False">
                <resource-adaptor-class-name>org.rockville.ra.AkkaResourceAdaptor</resource-adaptor-class-name>
            </resource-adaptor-class>
        </resource-adaptor-classes>
    </resource-adaptor>
</resource-adaptor-jar>
